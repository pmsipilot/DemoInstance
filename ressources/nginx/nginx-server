server {

       listen 8080;


       root /opt/demoinstance/frontend/;
       index index.html;

       location /api {
       		proxy_pass http://127.0.0.1:8081;
       }

       location / {
       		try_files $uri $uri/ =404;
       }

       location /instance_image/ {
       		alias /etc/demoinstance/instance_image/;
       		try_files $uri $uri/ =404;
       }


}

